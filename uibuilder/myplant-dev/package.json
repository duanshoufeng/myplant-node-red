{
  "name": "uib-blank",
  "version": "5.0.0",
  "private": true,
  "description": "This is about the simplest template you can get for uibuilder.",
  "browser": "./src/index.js",
  "scripts": {
    "start": "concurrently npm:watch:*",
    "watch:css": "tailwindcss build -i ./tailwind.css -o ./src/css/tailwind.css --watch",
    "watch:js": "npx webpack -w --config webpack.config.js"
  },
  "keywords": [
    "uibuilder",
    "node-red",
    "node-red-contrib-uibuilder"
  ],
  "author": "Julian Knight (Totally Information)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/TotallyInformation/node-red-contrib-uibuilder",
  "bugs": "https://github.com/TotallyInformation/node-red-contrib-uibuilder/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/TotallyInformation/node-red-contrib-uibuilder.git"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/nprogress": "^0.2.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "tailwindcss": "^3.0.23",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0"
  }
}
